    "babel-preset-stage-1": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "body-parser": "^1.18.2",
    "cloudinary": "^1.9.1",
    "cookie-session": "^2.0.0-beta.3",
    "cors": "^2.8.4",
    "express": "^4.16.2",
